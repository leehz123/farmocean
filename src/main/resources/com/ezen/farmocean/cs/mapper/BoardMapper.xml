<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ezen.farmocean.cs.mapper.BoardMapper">

	<select id="getCateList" resultType="com.ezen.farmocean.cs.dto.Cate">				
    	SELECT * FROM cate_info Order by cate_idx
	</select>
	
	<select id="getBoardList" resultType="com.ezen.farmocean.cs.dto.CsBoard">				
    	SELECT * FROM board_notice Order by board_idx desc
	</select>
	
</mapper>